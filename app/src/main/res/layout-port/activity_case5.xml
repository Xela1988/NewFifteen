<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/relativeLayout"
    android:orientation="horizontal"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Case5"
    android:background="@drawable/sfondo02"
    tools:ignore="MissingDefaultResource">


    <TextView
        android:id="@+id/textView"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="20dp"
        android:text="Number of moves: 0"
        android:textAlignment="center"
        android:textColor="#000000"
        android:textSize="30dp" />

    <GridLayout
        android:id="@+id/gridLayout5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:columnCount="5"
        android:rowCount="5"
        android:layout_alignWithParentIfMissing="false"
        android:clickable="false"
        android:useDefaultMargins="false"
        android:layout_centerVertical="true"
        android:layout_centerHorizontal="true"
        android:backgroundTint="@android:color/transparent"
        >

        <Button
            android:id="@+id/button"
            style="@style/Widget.AppCompat.Button"
            android:layout_width="55dp"
            android:layout_row="0"
            android:layout_column="0"
            android:layout_gravity="center"
            android:background="@drawable/yellowbutton00"
            android:text="1"
            android:textSize="24sp" />

        <Button
            android:id="@+id/button2"
            style="@style/Base.Widget.AppCompat.Button"
            android:layout_width="55dp"
            android:layout_row="0"
            android:layout_column="1"
            android:layout_gravity="center"
            android:background="@drawable/yellowbutton00"
            android:text="2"
            android:textSize="24sp" />

        <Button
            android:id="@+id/button3"
            style="@style/Widget.AppCompat.Button"
            android:layout_width="55dp"
            android:layout_row="0"
            android:layout_column="2"
            android:layout_gravity="center"
            android:background="@drawable/yellowbutton00"
            android:text="3"
            android:textSize="24sp" />

        <Button
            android:id="@+id/button4"
            style="@style/Widget.AppCompat.Button"
            android:layout_width="55dp"
            android:layout_row="0"
            android:layout_column="3"
            android:layout_gravity="left|center"
            android:background="@drawable/yellowbutton00"
            android:text="4"
            android:textSize="24sp" />

        <Button
            android:id="@+id/button5"
            style="@style/Widget.AppCompat.Button"
            android:layout_width="55dp"
            android:layout_height="wrap_content"
            android:layout_row="0"
            android:layout_column="4"
            android:layout_alignTop="@+id/gridLayout5"
            android:layout_alignEnd="@+id/gridLayout5"
            android:layout_gravity="center"
            android:background="@drawable/yellowbutton00"
            android:text="5"
            android:textSize="24sp" />

        <Button
            android:id="@+id/button6"
            style="@style/Widget.AppCompat.Button"
            android:layout_width="55dp"
            android:layout_row="1"
            android:layout_column="0"
            android:layout_gravity="center"
            android:background="@drawable/yellowbutton00"
            android:text="6"
            android:textSize="24sp" />

        <Button
            android:id="@+id/button7"
            style="@style/Widget.AppCompat.Button"
            android:layout_width="55dp"
            android:layout_row="1"
            android:layout_column="1"
            android:layout_gravity="center"
            android:background="@drawable/yellowbutton00"
            android:text="7"
            android:textSize="24sp" />

        <Button
            android:id="@+id/button8"
            style="@style/Widget.AppCompat.Button"
            android:layout_width="55dp"
            android:layout_row="1"
            android:layout_column="2"
            android:layout_gravity="center"
            android:background="@drawable/yellowbutton00"
            android:text="8"
            android:textSize="24sp" />

        <Button
            android:id="@+id/button9"
            style="@style/Widget.AppCompat.Button"
            android:layout_width="55dp"
            android:layout_row="1"
            android:layout_column="3"
            android:layout_gravity="left|center"
            android:background="@drawable/yellowbutton00"
            android:text="9"
            android:textSize="24sp" />


        <Button
            android:id="@+id/button10"
            style="@style/Widget.AppCompat.Button"
            android:layout_width="55dp"
            android:layout_height="wrap_content"
            android:layout_row="1"
            android:layout_column="4"
            android:layout_below="@+id/button5"
            android:layout_alignEnd="@+id/gridLayout5"
            android:layout_gravity="center"
            android:background="@drawable/yellowbutton00"
            android:text="10"
            android:textSize="24sp" />

        <Button
            android:id="@+id/button11"
            style="@style/Widget.AppCompat.Button"
            android:layout_width="55dp"
            android:layout_row="2"
            android:layout_column="0"
            android:layout_gravity="center"
            android:background="@drawable/yellowbutton00"
            android:text="11"
            android:textSize="24sp" />

        <Button
            android:id="@+id/button12"
            style="@style/Widget.AppCompat.Button"
            android:layout_width="55dp"
            android:layout_row="2"
            android:layout_column="1"
            android:layout_gravity="center"
            android:background="@drawable/yellowbutton00"
            android:text="12"
            android:textSize="24sp" />

        <Button
            android:id="@+id/button13"
            style="@style/Widget.AppCompat.Button"
            android:layout_width="55dp"
            android:layout_row="2"
            android:layout_column="2"
            android:layout_gravity="center"
            android:background="@drawable/yellowbutton00"
            android:text="13"
            android:textSize="24sp" />

        <Button
            android:id="@+id/button14"
            style="@style/Widget.AppCompat.Button"
            android:layout_width="55dp"
            android:layout_row="2"
            android:layout_column="3"
            android:layout_gravity="left|center"
            android:background="@drawable/yellowbutton00"
            android:text="14"
            android:textSize="24sp" />

        <Button
            android:id="@+id/button15"
            style="@style/Widget.AppCompat.Button"
            android:layout_width="55dp"
            android:layout_height="wrap_content"
            android:layout_row="2"
            android:layout_column="4"
            android:layout_below="@+id/button10"
            android:layout_alignEnd="@+id/gridLayout5"
            android:layout_gravity="center"
            android:background="@drawable/yellowbutton00"
            android:text="15"
            android:textSize="24sp" />

        <Button
            android:id="@+id/button16"
            style="@style/Widget.AppCompat.Button"
            android:layout_width="55dp"
            android:layout_row="3"
            android:layout_column="0"
            android:layout_gravity="center"
            android:background="@drawable/yellowbutton00"
            android:text="16"
            android:textSize="24sp" />

        <Button
            android:id="@+id/button17"
            style="@style/Widget.AppCompat.Button"
            android:layout_width="55dp"
            android:layout_row="3"
            android:layout_column="1"
            android:layout_gravity="center"
            android:background="@drawable/yellowbutton00"
            android:text="17"
            android:textSize="24sp" />

        <Button
            android:id="@+id/button18"
            style="@style/Widget.AppCompat.Button"
            android:layout_width="55dp"
            android:layout_row="3"
            android:layout_column="2"
            android:layout_gravity="center"
            android:background="@drawable/yellowbutton00"
            android:text="18"
            android:textSize="24sp" />


        <Button
            android:id="@+id/button19"
            style="@style/Widget.AppCompat.Button"
            android:layout_width="55dp"
            android:layout_row="3"
            android:layout_column="3"
            android:layout_gravity="left|center"
            android:background="@drawable/yellowbutton00"
            android:text="19"
            android:textSize="24sp" />

        <Button
            android:id="@+id/button20"
            style="@style/Widget.AppCompat.Button"
            android:layout_width="55dp"
            android:layout_height="wrap_content"
            android:layout_row="3"
            android:layout_column="4"
            android:layout_below="@+id/button10"
            android:layout_alignEnd="@+id/gridLayout5"
            android:layout_gravity="center"
            android:background="@drawable/yellowbutton00"
            android:text="20"
            android:textSize="24sp" />

        <Button
            android:id="@+id/button21"
            style="@style/Widget.AppCompat.Button"
            android:layout_width="55dp"
            android:layout_row="4"
            android:layout_column="0"
            android:layout_gravity="center"
            android:background="@drawable/yellowbutton00"
            android:text="21"
            android:textSize="24sp" />

        <Button
            android:id="@+id/button22"
            style="@style/Widget.AppCompat.Button"
            android:layout_width="55dp"
            android:layout_row="4"
            android:layout_column="1"
            android:layout_gravity="center"
            android:background="@drawable/yellowbutton00"
            android:text="22"
            android:textSize="24sp" />

        <Button
            android:id="@+id/button23"
            style="@style/Widget.AppCompat.Button"
            android:layout_width="55dp"
            android:layout_row="4"
            android:layout_column="2"
            android:layout_gravity="center"
            android:background="@drawable/yellowbutton00"
            android:text="23"
            android:textSize="24sp" />


        <Button
            android:id="@+id/button24"
            style="@style/Widget.AppCompat.Button"
            android:layout_width="55dp"
            android:layout_height="wrap_content"
            android:layout_row="4"
            android:layout_column="3"
            android:layout_gravity="center"
            android:background="@drawable/yellowbutton00"
            android:text="24"
            android:textSize="24sp" />

        <Button
            android:id="@+id/button275"
            style="@style/Widget.AppCompat.Button"
            android:layout_width="55dp"
            android:layout_height="wrap_content"
            android:layout_row="4"
            android:layout_column="4"
            android:layout_below="@+id/button275"
            android:layout_alignEnd="@+id/gridLayout5"
            android:layout_gravity="center"
            android:background="@drawable/yellowbutton00"
            android:textSize="24sp" />
    </GridLayout>

</RelativeLayout>
